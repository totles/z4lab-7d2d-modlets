<configs>
	<!-- Bag mod -->
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='CarryCapacity']/@value">60</set>
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='BagSize']/@value">60</set>
	<!-- Jump over 2 blocks high -->
	<set xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='JumpStrength']/@value">0.2</set>
	<!-- Loot and player bag remain for longer -->
	<set xpath="/entity_classes/entity_class[@name='Backpack']/property[@name='TimeStayAfterDeath']/@value">1200</set>
	<set xpath="/entity_classes/entity_class[@name='DroppedLootContainer']/property[@name='TimeStayAfterDeath']/@value">240</set>
	<set xpath="/entity_classes/entity_class[@name='EntityLootContainerRegular']/property[@name='TimeStayAfterDeath']/@value">1200</set>
	<set xpath="/entity_classes/entity_class[@name='EntityLootContainerStrong']/property[@name='TimeStayAfterDeath']/@value">1200</set>
	<set xpath="/entity_classes/entity_class[@name='EntityLootContainerBoss']/property[@name='TimeStayAfterDeath']/@value">1200</set>
	<set xpath="/entity_classes/entity_class[@name='EntityLootContainerBandit']/property[@name='TimeStayAfterDeath']/@value">1200</set>
	<!-- Increase loot drop chance -->
	<set xpath="/entity_classes/entity_class[@name='zombieTemplateMale']/property[@name='LootDropProb']/@value">0.15</set>
	<!-- Increase bone harvesting -->
	<set xpath="/entity_classes/entity_class/drop[@event='Harvest' and @tag='butcherHarvest']/@count">3,5</set>
	<!-- How long the Z body remains -->
	<set xpath="/entity_classes/entity_class[@name='zombieTemplateMale']/property[@name='TimeStayAfterDeath']/@value">20</set>
	<!-- animalStag - default harvest = 30 -->
	<set xpath="/entity_classes/entity_class[@name='animalStag']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">75</set>
	<!-- animalDoe - default harvest = 20 -->
	<set xpath="/entity_classes/entity_class[@name='animalDoe']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">50</set>
	<!-- animalRabbit - default harvest = 10 -->
	<set xpath="/entity_classes/entity_class[@name='animalRabbit']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">10</set>
	<!-- animalChicken - default harvest = 10 -->
	<set xpath="/entity_classes/entity_class[@name='animalChicken']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">10</set>
	<!-- animalBear - default harvest = 50 -->
	<set xpath="/entity_classes/entity_class[@name='animalBear']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">240</set>
	<!-- animalWolf - default harvest = 25 -->
	<set xpath="/entity_classes/entity_class[@name='animalWolf']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">50</set>
	<!-- animalSnake - default harvest = 10 -->
	<set xpath="/entity_classes/entity_class[@name='animalSnake']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">10</set>
	<!-- animalBoar - default harvest = 30 -->
	<set xpath="/entity_classes/entity_class[@name='animalBoar']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">90</set>
	<!-- animalDireWolf - default harvest = 35 -->
	<set xpath="/entity_classes/entity_class[@name='animalDireWolf']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">70</set>
	<!-- animalBossGrace - default harvest = 100 -->
	<set xpath="/entity_classes/entity_class[@name='animalBossGrace']/drop[@name='foodRawMeat'][@tag='butcherHarvest']/@count">300</set>
	<!-- animalZombieBear - default harvest = 7 -->
	<set xpath="/entity_classes/entity_class[@name='animalZombieBear']/drop[@name='foodRottingFlesh'][@tag='butcherHarvest']/@count">35</set>
	<!-- animalZombieVulture - default harvest = 3 -->
	<set xpath="/entity_classes/entity_class[@name='animalZombieVulture']/drop[@name='foodRottingFlesh'][@tag='butcherHarvest']/@count">1</set>
	<!-- animalZombieDog - default harvest = 4 -->
	<set xpath="/entity_classes/entity_class[@name='animalZombieDog']/drop[@name='foodRottingFlesh'][@tag='butcherHarvest']/@count">8</set>
</configs>
